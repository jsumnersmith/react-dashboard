{
  "name": "react-test",
  "version": "0.0.1",
  "scripts": {
    "start": "node app.js",
    "dev": "npm run watch & npm run start",
    "build": "browserify public/js/app.js -o public/js/bundle.js -dv",
    "watch": "watchify public/js/app.js -o public/js/bundle.js -dv"
  },
  "browserify": {
    "transform": [
      "reactify"
    ]
  },
  "dependencies": {
    "browserify": "^12.0.1",
    "express": "^4.13.3",
    "fbjs": "^0.6.0",
    "flux": "^2.1.1",
    "less-middleware": "^2.0.1",
    "nunjucks": "^2.2.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "reactify": "^1.1.1",
    "watchify": "^3.6.1"
  }
}
